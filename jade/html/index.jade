doctype html
html
  head
    meta(charset='utf-8')
    title App
    link(rel='stylesheet', href='/css/app.css')
    link(rel='shortcut icon', type='image/ico', href='/favicon.ico')
    script(src='/js/lib/require.js', data-main='/js/app', async=true)
  body
    .container
      .list
        - for (var x = 0; x < 20; x++)
          - var d = parseInt((Math.random() * 10).toString().split('.')[0], 10) % 2
          div(class= d ? 'content item' : 'content item item_double' )
            .bar #{x}
